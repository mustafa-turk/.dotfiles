{
  "diffEditor.ignoreTrimWhitespace": false,
  "customizeUI.titleBar": "frameless",
  "customizeUI.statusBarPosition": "under-panel",
  "customizeUI.stylesheet": {
    "div.split-view-container > div > div > div.title.tabs.show-file-icons > div.tabs-and-actions-container.tabs-border-bottom > div.editor-actions": "display: none !important;",
    "div.tabs-and-actions-container.tabs-border-bottom > div.monaco-scrollable-element.mac > div.tabs-container": "display: flex !important;",
    "div.tabs-and-actions-container.tabs-border-bottom > div.monaco-scrollable-element.mac > div.tabs-container > .tab": "flex: 1 !important;"
  },
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
  },
  "editor.fontSize": 13,
  "editor.fontFamily": "Jetbrains Mono",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.autoIndent": "full",
  "editor.scrollbar.vertical": "hidden",
  "editor.lineNumbers": "off",
  "editor.lineDecorationsWidth": 5,
  "editor.guides.indentation": false,
  "editor.guides.highlightActiveIndentation":false,
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.overviewRulerBorder": false,
  "editor.parameterHints.enabled": false,
  "git.decorations.enabled": false,
  "editor.folding": false,
  "editor.glyphMargin": false,
  "editor.scrollBeyondLastLine": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordBasedSuggestions": false,
  "editor.wordWrap": "on",
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "files.defaultLanguage": "javascript",
  "files.trimTrailingWhitespace": true,
  "git.enableSmartCommit": true,
  "javascript.suggest.names": false,
  "prettier.jsxSingleQuote": true,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.fontFamily": "Martian Mono",
  "terminal.integrated.fontSize": 13,
  "window.autoDetectColorScheme": false,
  "workbench.activityBar.visible": false,
  "workbench.tree.indent": 15,
  "workbench.iconTheme": null,
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "none",
  "workbench.tree.renderIndentGuides": "always",
  "editor.inlineSuggest.enabled": true,
  "window.nativeTabs": true
  ,
  "editor.accessibilitySupport": "off",
  "breadcrumbs.enabled": false,
  "workbench.editor.decorations.badges": false,
  "window.commandCenter": true,
  "explorer.decorations.colors": false,
  "workbench.editor.decorations.colors": false,
  "workbench.statusBar.visible": false,
  "workbench.colorTheme": "JetBrains Rider Dark Theme",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#DF97BD",
    "editor.lineHighlightBackground": "#00000000",
    "editor.lineHighlightBorder": "#00000000"
  }
}
