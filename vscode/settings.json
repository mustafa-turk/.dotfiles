{
  "breadcrumbs.enabled": false,
  "customizeUI.titleBar": "inline",
  "customizeUI.listRowHeight": 23,
  "customizeUI.stylesheet": {
    ".editor-actions, .actions-container, .dragging-area-top": "display: none;",
    "body.inline-title-bar .monaco-workbench:not(.fullscreen) .sidebar .composite.title": "background: #01121F !important; border-bottom: 1px solid #122D42;",
    ".monaco-workbench .part.editor>.content .editor-group-container>.title:not(.tabs)": "background: #01121F !important; padding-bottom: 2px;",
    ".monaco-workbench .part>.content>.composite": "border-right: 1px solid #122D42;",
    ".monaco-pane-view .pane>.pane-body": "background: #01121F;",
    ".monaco-workbench .part.editor>.content .editor-group-container>.title>.label-container>.title-label": "display: flex; justify-content: center;",
    ".monaco-workbench .part.statusbar": "background: #01121F !important;"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.autoIndent": "full",
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true
  },
  "editor.lineHeight": 23,
  "editor.fontFamily": "JetBrains Mono",
  "editor.smoothScrolling": true,
  "editor.fontSize": 14.5,
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.overviewRulerBorder": false,
  "editor.parameterHints.enabled": false,
  "editor.scrollBeyondLastLine": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordBasedSuggestions": false,
  "editor.wordWrap": "on",
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "files.defaultLanguage": "javascript",
  "files.trimTrailingWhitespace": true,
  "git.enableSmartCommit": true,
  "javascript.suggest.names": false,
  "prettier.jsxSingleQuote": true,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.fontFamily": "Jetbrains Mono",
  "terminal.integrated.fontSize": 15,
  "window.autoDetectColorScheme": false,
  "window.titleBarStyle": "native",
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Night Owl (No Italics)",
  "workbench.editor.showTabs": false,
  "workbench.tree.indent": 15,
  "workbench.iconTheme": null,
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tree.renderIndentGuides": "always"
}
